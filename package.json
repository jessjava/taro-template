		"@tarojs/rn-runner": "2.2.1",
		"@tarojs/router": "2.2.1",
		"@tarojs/taro": "2.2.1",
		"@tarojs/taro-alipay": "2.2.1",
		"@tarojs/taro-h5": "2.2.1",
		"@tarojs/taro-redux-rn": "2.2.1",
		"@tarojs/taro-rn": "2.2.1",
		"@tarojs/taro-router-rn": "2.2.1",
		"@tarojs/taro-swan": "2.2.1",
		"@tarojs/taro-tt": "2.2.1",
		"@tarojs/taro-weapp": "2.2.1",
		"babel-runtime": "^6.26.0",
		"blueimp-md5": "^2.10.0",
		"nerv-devtools": "^1.5.7",
		"nervjs": "^1.5.7",
		"react": "16.3.1",
		"react-native": "0.55.4",
		"redux": "^4.0.0",
